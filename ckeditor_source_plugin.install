<?php
/**
 * @file
 * Install, update, and uninstall functions for the module.
 */

/**
 * Implements hook_enable().
 * Display a message after the module installation.
 */
function ckeditor_source_plugin_enable() {
  drupal_set_message(t('The CKEditor Source Plugin is now enable, please add the
   corresponding module on CKEditor toolbar.'));
}
